<header class="site-header">
  <div class="site-header-brand">
    <% if(theme.nav_name) { %>
      <span class="site-header-brand-title">
        <a href="/"><%- theme.nav_name %></a>
      </span>
    <% } %>
    <% if(theme.motto) { %>
      <span class="site-header-brand-motto"> | <%- theme.motto %></span>
    <% } %>
  </div>
  <div class="site-header-right">
    <nav class="site-header-navigation">
      <% for(let link of theme.nav) { %>
        <a href="<%- link.path %>" target="<%- link.path.startsWith('http') ? '_blank' : '_self' %>"><%= link.name%></a>
      <% } %>
    </nav>
    <div class="site-header-btn">
      <% if(theme.github) { %>
        <a href="<%- theme.github %>" target="_blank">
          <i class="fa fa-github-alt"></i>
        </a>
      <% } %>
      <a href="javascript:void(0);" id="site-search">
        <i class="fa fa-search"></i>
      </a>
    </div>
  </div>
</header>
<div id="site-process"></div>